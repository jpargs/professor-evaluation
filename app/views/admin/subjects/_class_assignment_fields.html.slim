div class="nested-fields"
  .row
    .col-md-7
      .form-group
        label 
          | Teacher
          =f.select(:teacher_id, options_from_collection_for_select(@teachers,:id,:name, f.object.teacher_id), {include_blank: '-- Select Teacher --'}, :id=>"teacher_id", :class =>"form-control chosen-select")
          /=f.select(:member_id, options_from_collection_for_select(@members,:id,:full_name, f.object.member_id), {include_blank: '-- Select Member --'}, :id=>"member_id", :class =>"form-control chosen-select member-select")
          =link_to_remove_association "",f, :type=>"button", :confirm=>"Are you sure?", :class=>"fa fa-minus-circle"
          span.error-msg.hidden
        